<?php
function users() {
	return "csubook2, Ryan Hahn 1,Jino Park 2";
}

function user_name($uid)
{
	switch($uid)
	{
		case 1: return "Ryan Hahn's public profile\n";
		case 2: return "Jino Park's public profile\n";
		default: return "Invalid user id.\n";
	}
}
if (isset($_REQUEST['uid'])){echo user_name(trim($_REQUEST['uid']));  }
if (isset($_REQUEST['users'])){echo users();}

?>
